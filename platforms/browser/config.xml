<?xml version='1.0' encoding='utf-8'?>
<widget id="kz.kazpoisk.kz" version="1.6.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0.1">
    <name>kazpoisk</name>
    <description>
        Ads of Kazakhstan and World.
    </description>
    <author email="astana7777777@gmail.com" href="http://kazpoisk.kz">
        Akan Dulat Software Developer
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="5000" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="FadeSplashScreenDuration" value="750" />
    <preference name="Fullscreen" value="false" />
</widget>
